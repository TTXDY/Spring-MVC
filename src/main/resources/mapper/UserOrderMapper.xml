<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aust.mvc.mapper.UserOrderMapper">

    <insert id="insertOrder">
        insert into user_order(userno, orderno, total_price)
        values(#{userno}, #{orderno}, #{price})
    </insert>
</mapper>